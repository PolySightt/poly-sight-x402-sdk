  "author": "PolySight Team",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/web3.js": "^1.87.6",
    "bn.js": "^5.2.1",
    "snarkjs": "^0.7.3",
    "circomlibjs": "^0.1.7",
    "@noble/curves": "^1.3.0"
